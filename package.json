{
  "name": "trustless-design",
  "version": "1.0.0",
  "description": "Landing page for Trustless Design auditing services.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "dev": "webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --disable-host-check --hot",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prod": "webpack-dev-server --config webpack/prod.config.js --host 0.0.0.0 --disable-host-check",
    "precommit": "lint-staged",
    "formatAll": "find ./src/ -name '*.js*' | xargs prettier --write --config ./.prettierrc --config-precedence file-override"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
  "contributors": [
    "Daniel Kmak",
    "Logan Saether"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.6",
    "babel-loader": "~7.1.5",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-react": "~6.24.1",
    "browserslist": "^4.0.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-hot-loader": "^1.4.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "font-awesome-loader": "^1.0.2",
    "html-loader": "^0.5.5",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.14.0",
    "react-hot-loader": "~4.3.4",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "react": "~16.4.2",
    "react-dom": "~16.4.2",
    "tether": "^1.4.4"
  },
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
}
